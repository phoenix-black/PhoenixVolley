<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="PxVolleyDialogTheme" parent="Theme.AppCompat.Light.Dialog.Alert">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:colorPrimary">#000000</item>
        <item name="android:colorAccent">#00ffff</item>
        <item name="android:textColorPrimary">#00ff00</item>
        <item name="android:textColorSecondary">#0000ff</item>
        <item name="android:textColor">#ff33</item>
        <item name="android:textViewStyle">@style/PxTextViewStyle</item>
        <item name="android:buttonStyle">@style/PxButtonStyle</item>
    </style>

    <style name="PxButtonStyle" parent="android:Widget.Button">
        <item name="android:background">@android:color/transparent</item>
        <item name="android:textColor">#00ffff</item>
    </style>

    <style name="PxTextViewStyle" parent="android:Widget.TextView">
        <item name="android:textColor">#0000ff</item>
    </style>


    <style name="IconTheme">
        <item name="icon_color">#00ff00</item>
    </style>
    
    <style name="PxLayoutStyle" parent="Theme.AppCompat.Light.Dialog.Alert">
        <item name="android:background">#ff33</item>
    </style>

    <declare-styleable name="IconsStyle">
        <attr name="icon_color" format="color" />
        <attr name="icon_size" format="dimension" />
    </declare-styleable>
</resources>